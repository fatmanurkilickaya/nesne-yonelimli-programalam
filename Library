using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace LibraryApp
{
    public class Library
    {
        string isim, adres;
        public void GetirKutuphane(string name, string address)
        {
            Malzemeler mlz = new Malzemeler();
            this.isim = mlz.KutuphaneIsimDonder("malzeme kutuphane ismi");
            Console.WriteLine(this.isim);
            
            this.isim = name;
            this.adres = address;
            Console.WriteLine($"kütüphanenin ismi {isim} dir.");
        }
    }
}
