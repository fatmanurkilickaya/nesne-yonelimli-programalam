using System;
using System.Collections.Generic;
using System.Linq;
using System.Runtime.Remoting.Metadata.W3cXsd2001;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp1
{
    public class Program
    {
        static void Main(string[] args)
        {
            Person p = new Person { name = "fatmanur", surname = "kılıçkaya" };

            p.GosterİsimSoyisim1();
            Console.ReadKey();
           
           

        }
    }

    public class Person
    {
        public string name, surname;

        public void GosterİsimSoyisim1()
        {
            Console.WriteLine($"isim {name} dir, soyisim {surname} dir....");
        }
        public void GosterİsimSoyisim12()
        {
            Console.WriteLine($"isim {name} soyisim {surname} ");
        }
    }
}
